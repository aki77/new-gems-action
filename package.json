{
  "name": "new-gems-action",
  "version": "2.2.0",
  "private": true,
  "type": "module",
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "biome format --write .",
    "format-check": "biome format --check .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --fix .",
    "check": "biome check .",
    "check:fix": "biome check --fix .",
    "pack": "ncc build",
    "all": "npm run build && npm run check && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aki77/new-gems-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "aki77",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@aki77/actions-replace-comment": "^0.6.0",
    "got": "14.4.7",
    "markdown-table": "^3.0.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/markdown-table": "^3.0.0",
    "@types/node": "^20.19.9",
    "@vercel/ncc": "^0.38.3",
    "js-yaml": "^4.1.0",
    "typescript": "^5.8.3"
  }
}
