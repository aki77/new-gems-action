{
  "name": "new-gems-action",
  "version": "2.1.0",
  "private": true,
  "type": "module",
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "all": "npm run build && npm run format && npm run lint && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aki77/new-gems-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "aki77",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^6.0.0",
    "@aki77/actions-replace-comment": "^0.6.0",
    "got": "14.4.2",
    "markdown-table": "^3.0.0"
  },
  "devDependencies": {
    "@types/markdown-table": "^3.0.0",
    "@types/node": "^20.16.10",
    "@typescript-eslint/parser": "^8.8.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.1",
    "eslint-plugin-github": "^5.0.1",
    "eslint-plugin-jest": "^28.8.1",
    "eslint-plugin-prettier": "^5.2.1",
    "js-yaml": "^4.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
